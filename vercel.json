{
  "rewrites": [
    {
      "source": "/api/deposit",
      "destination": "/api/wallet?action=deposit"
    },
    {
      "source": "/api/withdraw",
      "destination": "/api/wallet?action=withdraw"
    },
    {
      "source": "/api/history-analysis",
      "destination": "/api/history?action=analysis"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "framework": "vite",
  "buildCommand": "vite build",
  "devCommand": "vite --port $PORT",
  "installCommand": "npm install",
  "outputDirectory": "dist"
}
