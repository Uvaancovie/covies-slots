{
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://covies-slots.onrender.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "vite build",
  "devCommand": "vite --port $PORT",
  "installCommand": "npm install",
  "outputDirectory": "dist"
}
